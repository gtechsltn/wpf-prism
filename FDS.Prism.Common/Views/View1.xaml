<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:FDS.Prism.Common.Views">
    <Style TargetType="views:View1">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="views:View1">
                    <StackPanel Orientation="Vertical">
                        <TextBlock Text="{Binding Path=ViewName}"/>
                        <Button Command="{Binding GotoView2Command}" Content="Go to View 2"/>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
